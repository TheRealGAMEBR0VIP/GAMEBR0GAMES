     size 1 30
     valign center_ref
     vexactsize 1
     text "{LABEL}"
     font "gui/fonts/sdf_MetronLight24"
     "text offset" 10 0
     "bold text" 1
     "exact text" 1
     "exact text size" 16
     "text valign" center
    }
    FrameWidgetClass action {
     ignorepointer 1
     position 0 0
     size 0.7 1
     halign right_ref
     valign center_ref
     hexactpos 0
     vexactpos 0
     hexactsize 0
     vexactsize 0
    }
   }
  }
  PanelWidgetClass action_wrapper_disable {
   visible 0
   clipchildren 0
   ignorepointer 0
   color 0.1569 0.1569 0.1569 0.902
   position 0 0
   size 1 1
   style rover_sim_colorable
  }
 }
}