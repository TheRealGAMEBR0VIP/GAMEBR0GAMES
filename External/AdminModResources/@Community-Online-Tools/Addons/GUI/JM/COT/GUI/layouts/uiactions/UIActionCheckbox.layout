     font "gui/fonts/sdf_MetronLight24"
     "text offset" 0 0
     "bold text" 1
     "exact text" 1
     "exact text size" 16
     "text valign" center
    }
   }
  }
  PanelWidgetClass action_wrapper_disable {
   visible 0
