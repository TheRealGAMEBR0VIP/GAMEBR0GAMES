PanelWidgetClass UIActionText {
 size 1 30
 halign left_ref
 valign top_ref
 hexactpos 0
 vexactpos 0
 hexactsize 0
 vexactsize 1
 color 0 0 0 0
 style rover_sim_colorable
 scriptclass "UIActionText"
 {
  FrameWidgetClass action_wrapper {
   ignorepointer 1
   position 0 0
   size 1 1
   hexactpos 0
   vexactpos 0
   hexactsize 0
   vexactsize 0
   {
    TextWidgetClass action_label {
     size 1 20
     valign center_ref
     vexactsize 1
     text "{LABEL}"
     font "gui/fonts/sdf_MetronLight24"
     "text offset" 10 0
     "bold text" 1
     "exact text" 1
     "exact text size" 16
    }
    TextWidgetClass action {
     size 1 20
     halign right_ref
     valign center_ref
     vexactsize 1
     text "{TEXT}"
     font "gui/fonts/sdf_MetronLight24"
     "text offset" -10 0
     "exact text" 1
     "exact text size" 16
     "text halign" right
    }
   }
  }
  PanelWidgetClass action_wrapper_disable {
   visible 0
   clipchildren 0
   ignorepointer 0
   halign center_ref
   valign center_ref
   color 0.06 0.08 0.11 0.996
   position 0 0
   size 1.1 1.1
   style rover_sim_colorable
  }
 }
}